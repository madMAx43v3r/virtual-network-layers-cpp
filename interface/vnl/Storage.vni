package vnl;

class Entry {
	
	Hash64 key;
	long version;
	
}

interface Storage {
	
	void put_entry(Pointer<Entry> value);
	
	Pointer<Entry> get_entry(Hash64 key) const;
	
	void delete_entry(Hash64 key);
	
}

object StorageServer implements Storage {
	
	String filename;
	
	int interval = 1000000;
	
	List<Address> topics;
	
	void handle(Entry sample);
	
}
